 <h2> New Wiki</h2>
 <div class= "row">
  <%= form_for @wiki do |f| %>
   <div class="col-md-12">
    <div class="form-group">
     <%= f.label :title %>
     <%= f.text_field :title %>
    </div>
    <div class="form-group">
     <%= f.label :body %>
     <%= f.text_area :body %>
     </div>
     <div class="form-group">
     <%= f.label :private %>
     <%= f.check_box :private %>
    </div>
    <div class="form-group">
     <%= f.submit "Save" %>
     </div>
   </div>
   <% end %>
 </div>